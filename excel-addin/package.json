{
  "name": "gridmate-excel-addin",
  "version": "0.1.0",
  "description": "Gridmate Excel Add-in - AI-powered financial modeling assistant",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "dev-server": "vite",
    "start": "office-addin-debugging start public/manifest.xml",
    "start:desktop": "office-addin-debugging start public/manifest.xml desktop",
    "start:web": "office-addin-debugging start public/manifest.xml web",
    "stop": "office-addin-debugging stop public/manifest.xml",
    "sideload": "office-addin-debugging start public/manifest.xml",
    "validate": "office-addin-manifest validate public/manifest.xml",
    "certificates": "office-addin-dev-certs install"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@microsoft/office-js": "^1.1.85",
    "@microsoft/signalr": "^8.0.7",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/uuid": "^10.0.0",
    "axios": "^1.6.7",
    "formulajs": "^1.0.8",
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.6.1",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/office-js": "^1.0.516",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "http-proxy-middleware": "^3.0.5",
    "jest": "^29.7.0",
    "office-addin-debugging": "^6.0.3",
    "office-addin-dev-certs": "^2.0.3",
    "office-addin-dev-settings": "^3.0.3",
    "office-addin-manifest": "^2.0.3",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.3"
  }
}
