{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/_next/*"
    },
    {
      "route": "/api/*",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "route": "/*",
      "methods": ["GET"],
      "rewrite": "/index.html"
    }
  ],
  "platform": {
    "apiRuntime": "node:18"
  },
  "trailingSlash": "auto"
}